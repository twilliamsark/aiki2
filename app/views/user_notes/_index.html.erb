<%
  notes = current_user.user_notes.where(waza_format: waza_format)
  if notes.any?
%>
  <h4 class="pad_sides">User Notes</h4>
  <div class="row pad_sides">
    <div class="col-md-12">
      <table class="table table-striped table-bordered">
        <tbody>
          <%= render notes, waza_format: waza_format, video: video %>
        </tbody>
      </table>
    </div>
  </div>
<% end %>
