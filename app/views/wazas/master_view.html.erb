<% provide(:title, @title) %>
<%
  @format_names = Format.all.sort_by(&:position).map(&:name)
%>
<% unless @recent_master_hash.empty? %>
  <%= render 'master_grid', format_names: @format_names, master_hash: @recent_master_hash, recent_only: true %>
  <br/>
<% end %>
<%= render 'master_grid', format_names: @format_names, master_hash: @master_hash %>
