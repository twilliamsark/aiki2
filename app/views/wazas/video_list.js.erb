$("#video_list").replaceWith("<%= escape_javascript(render('video_list', selection: @selection, keys: @keys, sort_by: @default_sort, type: @type)) %>")
<% if @waza
  at = exhibit(@waza, self)
%>
  $("#vid").replaceWith("<%= escape_javascript(at.render_body(@waza_format, @video)) %>")
<% else %>
  $("#vid").replaceWith("<%= escape_javascript(render('shared/no_video')) %>")
<% end %>
$("#filter_form").find('input[type=text]').val('')
