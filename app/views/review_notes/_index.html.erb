<%
  notes = waza_format.review_notes rescue []
  if notes.any?
%>
  <h4 class="pad_sides">Review Notes</h4>
  <div class="row pad_sides">
    <div class="col-md-12">
      <table class="table table-striped table-bordered">
        <tbody>
          <%= render partial: "user_notes/user_note", collection: notes, as: :note, locals: {waza_format: waza_format} %>
        </tbody>
      </table>
    </div>
  </div>
<% end %>
